@{
    ViewBag.Title = "Schedule --Service Speaks --Steans Center";
    Layout = "~/Areas/ServiceSpeaks/Views/Shared/_Layout.cshtml";
}
@section Head
{
    <style type="text/css">
        section{ height: 500px;padding-top: 20px;}
        span.imgspan{ float: left;}
        h1,p{ width: 100%;display: block;}
        section div h1{ color: #252543;}
        b{ font-size: 16px;}
        table{ width: 100%;}
        table td{ padding: 5px;}
        table tr:nth-child(odd) td{ background-color: #aaaaaa;}
    </style>
}
<section>
    <div class="grid-c2-w15e9">
        <div class="west">
            <h1>FRIDAY, MAY 9TH, 2014</h1>
            <em>*Elements of the schedule are subject to change*</em>
            <table>
                <tr>
                    <td>9:30am- 9:45am</td>
                    <td>Registration (2250 N. Sheffield, Chicago, IL 60614) 3rd Floor</td>
                </tr>
                <tr>
                    <td>10:00am- 10:55am</td>
                    <td>Session I</td>
                </tr>
                <tr>
                    <td>11:00am- 11:55am</td>
                    <td>Session II</td>
                </tr>
                <tr>
                    <td>12:00pm- 1:00pm</td>
                    <td>Session III</td>
                </tr>
                <tr>
                    <td>1:05pm- 2:00pm</td>
                    <td>Session IV</td>
                </tr>
                <tr>
                    <td>2:05pm- 3:00pm</td>
                    <td>Session IV</td>
                </tr>
            </table>
        </div>
        <div class="east">
            @Html.Partial("RightNav")
        </div>
    </div>

</section>
