body, header, footer, figure, section, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, p, blockquote, th, td {
    margin: 0;
    padding: 0;
    width: auto;
}

ol, ul, li {
    list-style: none;
}

address {
    font-style: normal;
    font-weight: normal;
}

img {
    border: 0;
    margin: 0;
}

caption, th {
    text-align: left;
}

q:before, q:after {
    content: '';
}


html {
    text-align: center;
}

body {
    background-color: #EDEAD1;
    font-family: 'Droid Sans','Helvetica Neue',Arial,'Liberation Sans',FreeSans,sans-serif;
    font-size: 12px;
    line-height: 25px;
    text-align: left;
}

a {
    font-family: 'Droid Sans','Helvetica Neue',Arial,'Liberation Sans',FreeSans,sans-serif;
}

table {
    width: 100%;
    border-collapse: collapse;
    border-spacing: 0;
    margin: 20px;
    background: #ffffff;
}
th {
    background-image: url(/images/barbg3.jpg);
    height: 28px;
    text-align: center;
    font-weight: bold;
    border: solid 1px #CCC;
}

td {
    border: solid 1px #ccc;
    text-align: center;
}

tr:nth-child(even) {
    background: #f9f5f6;
}

tr:not(:first-child):hover {
    background: rgb(193, 213, 227);
    color: #FFF;
}

header {
    width: 100%;
    background: #006AAD;
    height: 86px;
    color: #fff;
    position: relative;
    float: left;
}

    header a {
        color: #ffffff;
    }

    header div#top {
        height: 50px;
    }

    header figure {
        position: absolute;
        left: 20px;
        top: 10px;
        font-size: 20px;
        font-weight: bold;
    }

    header aside {
        position: absolute;
        right: 20px;
    }

    header div#bottom div {
        width: 70%;
        float: left;
        background-image: url(/images/barbg.jpg);
        height: 34px;
        line-height: 34px;
        border-bottom: 1px solid rgb(51, 51, 51);
        font-size: 15px;
        font-weight: bold;
    }

    header div#bottom ul {
        background: #006AAD;
        width: 30%;
        float: left;
        border-bottom: 1px solid rgb(51, 51, 51);
        height: 34px;
    }

        header div#bottom ul li {
            float: left;
            color: #000000;
            font-size: 13px;
            font-weight: 500;
            line-height: 35px;
            list-style-type: none;
            padding-left: 4px;
            padding-right: 4px;
        }

            header div#bottom ul li#barright {
                padding: 0;
                margin: 0;
            }

            header div#bottom ul li a {
                cursor: pointer;
                margin-right: 5px;
                vertical-align: middle;
            }

section#main {
    width: 100%;
    float: left;
}

    section#main div#left {
        width: 25%;
        float: left;
    }
    section#main div#left .container {
        width: 240px;
        background: none repeat scroll 0 0 #EEEEEE;
    }
        section#main div#left h1 {
            text-align: center;
        }

        section#main div#left nav {
            background: none repeat scroll 0 0 #F0F8FF;
            margin: 0;
            padding: 0;
            width: 240px;
        }

    section#main div#right {
        float: left;
        width: 70%;
    }

        section#main div#right div.nav {
            background: #F8F8F8;
            border: solid 1px #CCC;
            border-radius: 4px;
            display: block;
            min-height: 50px;
            margin: 20px;
        }

            section#main div#right div.nav h1 {
                line-height: 40px;
                padding: 5px 15px 5px 15px;
            }
footer {
    margin-top: 50px;
    border-top: solid 1px #aaa;
    text-align: center;
    min-height: 100px;
    float: left;
    width: 100%;
}